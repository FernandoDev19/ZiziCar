model ZIC_ADM_BRANDS {
  id                String    @id @default(dbgenerated("(uuid())"))
  name              String
  doors             Int
  positions         Int
  generic_image     String
  large_suitcases   Int?
  small_suitcases   Int?
  cylinder_capacity Decimal   @db.Decimal(10, 0)
  created_at        DateTime  @default(now()) @db.Timestamp(0)
  updated_at        DateTime? @db.Timestamp(0)
}
